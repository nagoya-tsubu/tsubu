<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.androidtsubu.ramentimer">
  <application android:icon="@drawable/icon" android:debuggable="true">
    <activity android:name=".DashboardActivity" android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name="TimerActivity" />
    <activity android:name="CreateActivity" />
    <activity android:name="HistoryActivity" />
    <activity android:name="ReaderActivity" />
    <service android:enabled="true" android:name=".RamenTimerService"/>
  </application>


<uses-sdk android:minSdkVersion="7" android:targetSdkVersion="7"></uses-sdk>
</manifest>